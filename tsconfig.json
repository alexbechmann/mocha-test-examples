{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "preserveSymlinks": true,
    "rootDir": "../",
    "declaration": true
  }
}
